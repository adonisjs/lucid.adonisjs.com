    "@dimerapp/docs-theme": "^6.1.3",
    "@dimerapp/edge": "^5.0.0",
    "@docsearch/css": "^4.6.0",
    "@docsearch/js": "^4.6.0",
    "@poppinss/ts-exec": "^1.4.4",
    "@radix-ui/colors": "^3.0.0",
    "@swc/core": "^1.15.13",
    "@types/node": "^25.3.1",
    "alpinejs": "^3.15.8",
    "collect.js": "^4.36.1",
